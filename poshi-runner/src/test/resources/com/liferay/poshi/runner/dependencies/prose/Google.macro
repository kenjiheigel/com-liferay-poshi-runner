<definition>
	<command matching-string="I open the URL ${url}" name="open">
		<execute function="Open" locator1="${url}" />
	</command>

	<command matching-string="I search for ${input}" name="search">
		<execute function="Type" locator1="//input[@title='Search']" value1="${input}" />
		<execute function="Click" locator1="//input[@title='Search']" />
	</command>

	<command matching-string="I should see ${assert}" name="assertSearch">
		<execute function="AssertTextPresent" locator1="//body" value1="${assert}" />
	</command>
</definition>